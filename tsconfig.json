{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "allowJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "skipLibCheck": true,
    "declaration": true,
    "downlevelIteration": true
  }
}